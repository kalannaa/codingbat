public int[] front11(int[] a, int[] b) {
  int[] array = new int[2];
  int[] array2 = new int[1];
  
  if (a.length >=1 && b.length >= 1) {
    array[0] = a[0];
    array[1] = b[0];
    return array;
  } else if (a.length >=1 && b.length == 0) {
    array2[0] = a[0];
    return array2;
  } else if (a.length ==0 && b.length >= 1) {
    array2[0] = b[0];
    return array2;
  }
    int[] array3 = new int[0];
    return array3;
}
