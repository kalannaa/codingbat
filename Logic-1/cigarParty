public boolean cigarParty(int cigars, boolean isWeekend) {
  //weekday, 40-60 cigars - success
  //weekend, min. 40 cigars - success
  
  if(isWeekend == true && cigars >=40) {
      return true;
  } else if (isWeekend == false && (cigars <=60) && (cigars >= 40 )) {
      return true;
    } else return false;
  
}
